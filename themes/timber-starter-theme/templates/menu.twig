{% if wp.menus['main'] %}
	<ul>
	<div class="main-menu">
		{% for item in items %}
			<li class="{{ item.classes | join(' ') }}">
				<a target="{{ item.target }}" href="{{ item.link }}">{{ item.title }}</a>
			</li>
		{% endfor %}
	</div>
	<li>
		<a class="logo-link" href="{{site.url}}" rel="home">MR</a>
	</li>

	</ul>
{% endif %}
